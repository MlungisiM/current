<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Cregalink Regression Test Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="configurations.ExtentTestListener"/>
    </listeners>
    <test name="Login Tests">
        <classes>
            <class name="tests.login_tests"/>
        </classes>
    </test>

    <test name="DAA Submission Tests">
        <classes>
            <class name="tests.daa_submissions_tests"/>
        </classes>
    </test>
</suite>

